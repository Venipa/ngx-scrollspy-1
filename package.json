{
  "name": "ngx-scrollspy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=TestCases --host=0.0.0.0",
    "build": "ng build",
    "build:testcases": "ng build --project=TestCases --prod",
    "build:development": "ng build --watch",
    "test": "ng test ngx-scrollspy --watch=false --code-coverage",
    "test:travis": "ng test ngx-scrollspy --watch=false --code-coverage --no-progress --browsers=ChromeHeadlessNoSandbox",
    "test:watch": "ng test ngx-scrollspy --watch",
    "lint": "ng lint ngx-scrollspy",
    "publish": "cd dist/ngx-scrollspy && npm publish --access public",
    "test:changelog": "changelog -M",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "core-js": "^2.5.4",
    "prismjs": "^1.16.0",
    "rxjs": "~6.5.4",
    "stickyfilljs": "^2.1.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.4",
    "codelyzer": "^5.0.1",
    "fstream": "^1.0.12",
    "generate-changelog": "^1.8.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
